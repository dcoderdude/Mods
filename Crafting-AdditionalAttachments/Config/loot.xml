<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<!-- Add Incendiary Rounds to Loot -->
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEm']">
		<item name="modGunIncendiaryRounds"/>
	</insertAfter>
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEmSchematic']">
		<item name="modGunIncendiaryRoundsSchematic"/>
	</insertAfter>
  
  <!-- Add Shocking Rounds to Loot -->
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEm']">
		<item name="modGunShocked"/>
	</insertAfter>
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEmSchematic']">
		<item name="modGunShockedSchematic"/>
	</insertAfter>
  
  <!-- Add Blessed Metal Rounds to Loot -->
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEmSchematic']">
		<item name="modBlessedMetal"/>
	</insertAfter>
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEm']">
		<item name="modBlessedMetalSchematic"/>
	</insertAfter>
  
  <!-- Add Ice Rounds to Loot -->
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEmSchematic']">
		<item name="modGunLiquidNitrogen"/>
	</insertAfter>
	<insertAfter xpath="/lootcontainers/lootgroup/item[@name='modGunCrippleEm']">
		<item name="modGunLiquidNitrogenSchematic"/>
	</insertAfter>
</configs>
