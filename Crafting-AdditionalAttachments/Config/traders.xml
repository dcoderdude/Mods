<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<!-- Add Incendiary Rounds to Trader-->
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEm']">
		<item name="modGunIncendiaryRounds"/>
	</insertAfter>
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEmSchematic']">
		<item name="modGunIncendiaryRoundsSchematic"/>
	</insertAfter>
  
	<!-- Add Shocking Rounds to Trader -->
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEm']">
		<item name="modGunShocked"/>
	</insertAfter>
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEmSchematic']">
		<item name="modGunShockedSchematic"/>
	</insertAfter>
  
	<!-- Add Blessed Metal Rounds to Trader -->
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEm']">
		<item name="modBlessedMetal"/>
	</insertAfter>
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEmSchematic']">
		<item name="modBlessedMetalSchematic"/>
	</insertAfter>
  
	<!-- Add Ice Rounds to Trader -->
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEm']">
		<item name="modGunLiquidNitrogen"/>
	</insertAfter>
	<insertAfter xpath="/traders/trader_item_groups/trader_item_group/item[@name='modGunCrippleEmSchematic']">
		<item name="modGunLiquidNitrogenSchematic"/>
	</insertAfter>
</configs>